apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    uid: Prometheus
    access: proxy
    url: http://192.168.2.102:9090
    isDefault: true
    editable: false
    jsonData:
      timeInterval: 15s
      queryTimeout: 60s
      httpMethod: POST
    version: 1

  - name: Loki
    type: loki
    uid: Loki
    access: proxy
    url: http://192.168.2.101:3100
    isDefault: false
    editable: false
    jsonData:
      maxLines: 1000
      timeout: 60s
    version: 1
